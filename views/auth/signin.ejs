<section class="container">
    <div class="section-title">
        <h2 data-aos="fade-in"> sign in </h2>
      </div>
      <form method="POST" action="/auth/signin">
        <div class="row mb-3">
            <div class="col-12">
              <label>Phone Number</label>
              <input name="phoneNumber" class="form-input" placeholder="+966"/>
            </div>
          </div>
          <div class="row mb-3">
              <div class="col-12">
                <label>Password</label>
                <input name="password" class="form-input" placeholder="*******" />
              </div>
            </div>
        <div class="row mb-3">
            <div class="col-12">
                <a id="changePass" href="/auth/changepass">Change password</a>
            </div>
          </div>
        <button class="btn-get-started" type="submit">Sign In</button>
      </form>
  </section>